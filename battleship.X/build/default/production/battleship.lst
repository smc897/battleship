                      00248 ; General formats are shown below in relocatible format.
                      00249 ;
                      00250 ;------------------------------PIC16's and below--------------------------------
                      00251 ;
                      00252 ; ISR       CODE    0x0004           ; interrupt vector location
                      00253 ;
                      00254 ;     <Search the device datasheet for 'context' and copy interrupt
                      00255 ;     context saving code here.  Older devices need context saving code,
                      00256 ;     but newer devices like the 16F#### don't need context saving code.>
                      00257 ;
                      00258 ;     RETFIE
                      00259 ;
                      00260 ;----------------------------------PIC18's--------------------------------------
                      00261 ;
                      00262 ; ISRHV     CODE    0x0008
                      00263 ;     GOTO    HIGH_ISR
                      00264 ; ISRLV     CODE    0x0018
                      00265 ;     GOTO    LOW_ISR
                      00266 ;
                      00267 ; ISRH      CODE                     ; let linker place high ISR routine
                      00268 ; HIGH_ISR
                      00269 ;     <Insert High Priority ISR Here - no SW context saving>
                      00270 ;     RETFIE  FAST
                      00271 ;
                      00272 ; ISRL      CODE                     ; let linker place low ISR routine
                      00273 ; LOW_ISR
                      00274 ;       <Search the device datasheet for 'context' and copy interrupt
                      00275 ;       context saving code here>
                      00276 ;     RETFIE
                      00277 ;
                      00278 ;*******************************************************************************
